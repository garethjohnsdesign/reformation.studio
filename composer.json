{ 
  "name": "garethjohnsdesign/craft",
  "description": "Gareth Johns Design Craft 3 CMS scaffolding project",
  "version": "1.0.1",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "info@garethjohnsdesign.com",
    "issues": "https://github.com/nystudio107/craft/issues",
    "source": "https://github.com/nystudio107/craft",
    "docs": "https://github.com/nystudio107/craft"
  },
  "require": {
    "craftcms/cms": "3.2.0-RC3",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/contact-form": "^2.2",
    "rias/craft-position-fieldtype": "1.0.14",
    "craftcms/google-cloud": "1.3.0",
    "aelvan/imager": "v2.1.10",
    "nystudio107/craft-minify": "1.2.9",
    "craftcms/redactor": "2.3.3.2",
    "verbb/field-manager": "2.1.0",
    "carlcs/craft-redactorcustomstyles": "3.0.2",
    "verbb/super-table": "2.1.20"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
